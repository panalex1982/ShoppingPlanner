<?xml version="1.0" encoding="utf-8"?>
<com.bue.shoppingplanner.free.views.other.HiddingTableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/hidePriceEditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_span="2"
        android:ems="10"
        android:inputType="numberDecimal" >

        <requestFocus />
    </EditText>

    <ImageView
        android:id="@+id/hideAcceptImageView"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_margin="5dp"
        android:src="@drawable/ic_check_ok" />

    <ImageView
        android:id="@+id/hideCancelImageView"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_margin="5dp"
        android:src="@drawable/ic_check_cancel" />

</com.bue.shoppingplanner.free.views.other.HiddingTableRow>